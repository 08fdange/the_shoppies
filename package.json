{
  "name": "the_shoppies",
  "version": "1.0.0",
  "description": "App nominating your 5 favorite movies or tv shows, built with React, Express and MongoDB.",
  "private": false,
  "license": "MIT",
  "browserslist": [
    "last 7 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "main": "src/index.tsx",
  "prox": "http://localhost:4001",
  "scripts": {
    "build": "node ./buildScript",
    "build-front": "react-scripts build",
    "eject": "react-scripts eject",
    "start-server": "nodemon server/index.js --watch index/*",
    "start-front": "react-scripts start",
    "start": "concurrently \"npm run start-server\" \"npm run start-front\" --kill-others --kill-others-on-fail",
    "test": "react-scripts test --env=jsdom"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.13.5",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "compression": "1.7.4",
    "connect-ensure-login": "^0.1.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "4.17.1",
    "helmet": "3.22.0",
    "knex": "0.20.13",
    "mongoose": "^5.11.9",
    "path": "^0.12.7",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "concurrently": "5.1.0",
    "nodemon": "2.0.3",
    "react-scripts": "4.0.1",
    "sqlite3": "4.1.1"
  }
}
